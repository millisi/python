def branch ():
   line(0,0,100,0)
   line(70,0,90,20)
   line(70,0,90,-20)
   line (30,0,50,20)
   line (30,0,50,-20)
def snowflake():
    for i in range (8):
        branch ()
        rotate(radians(45))
  
    
def setup ():
    size (500,500)
def draw():
    translate(250,250)
    snowflake ()
        
