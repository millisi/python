def branch ():
   line(0,0,100,0)
   line(70,0,90,20)
   line(70,0,90,-20)
   line (30,0,50,20)
   line (30,0,50,-20)
def snowflake(x,y,scale):
    for i in range (8):
        branch ()
        rotate(radians(45))

x=200
y=200
spreed = 5

def setup ():
    size (500,500)
def draw():
    global x ,y,scale,spreed 
    translate(250,250)
    snowflake (250,250,scale(2))
